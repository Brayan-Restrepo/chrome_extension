  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/ojo.png",
      "32": "img/ojo.png",
      "48": "img/ojo.png",
      "128": "img/ojo.png"
    }
  },
  "icons": {
    "16": "img/ojo.png",
    "32": "img/ojo.png",
    "48": "img/ojo.png",
    "128": "img/ojo.png"
  },
  "manifest_version": 2
}