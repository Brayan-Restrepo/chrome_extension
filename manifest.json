{
  "name": "Manupular Dom",
  "version": "1.1",
  "description": "Description",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js" , "function.js"],
    "persistent": false
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "MacCtrl+Shift+A"
       },
      "description": "Send a 'toggle-feature' event to the extension"
    }
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/ojo.png",
      "32": "img/ojo.png",
      "48": "img/ojo.png",
      "128": "img/ojo.png"
    }
  },
  "icons": {
    "16": "img/ojo.png",
    "32": "img/ojo.png",
    "48": "img/ojo.png",
    "128": "img/ojo.png"
  },
  "manifest_version": 2
}